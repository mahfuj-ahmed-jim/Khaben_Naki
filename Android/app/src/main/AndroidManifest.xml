<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.khabennaki">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KhabenNaki"
        android:usesCleartextTraffic="true">
        <activity android:name=".Design.SignUp.InformationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Design.Home.HomeActivity" />
        <activity android:name=".Design.SignUp.PinCodeActivity" />
        <activity android:name=".Design.SignUp.SignInActivity" />
        <activity android:name=".Design.SignUp.SelectActivity" />
        <activity android:name=".Design.SignUp.MainActivity">

        </activity>
    </application>

</manifest>